{
  "name": "sdk-fullredis",
  "version": "1.1.0",
  "description": "SDK TypeScript completo para Redis Full Gateway API com funcionalidades de IA integradas",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rmdir /s /q dist 2>nul || echo \"dist folder cleaned\"",
    "dev": "bun run --watch src/main.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepack": "npm run build",
    "type-check": "bun tsc --noEmit"
  },
  "keywords": [
    "redis",
    "sdk",
    "typescript",
    "api-client",
    "ai",
    "workflow",
    "cache",
    "database",
    "fullredis",
    "gateway"
  ],
  "author": "suissa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suissa/sdk-fullredis.git"
  },
  "homepage": "https://github.com/suissa/sdk-fullredis#readme",
  "bugs": {
    "url": "https://github.com/suissa/sdk-fullredis/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@vitest/ui": "^3.2.4",
    "axios-mock-adapter": "^2.1.0",
    "ts-node": "^10.9.2",
    "vitest": "^3.2.4"
  }
}